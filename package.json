{
  "name": "drf-js",
  "version": "0.0.0",
  "description": "A Django Rest Framework js wrapper library",
  "main": "drf.js",
  "scripts": {
    "test": "npm run eslint; npm run jscs; npm run karma",
    "eslint": "eslint src tests",
    "jscs": "jscs src tests",
    "karma": "karma start tests/karma.conf.js",
    "karma:watch": "karma start --auto-watch=true --single-run=false",
    "watchify": "watchify ./src/main.js -v -t [ babelify --presets [ es2015 ] ] -o ./drf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incuna/drf-js.git"
  },
  "author": "Incuna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/incuna/drf-js/issues"
  },
  "homepage": "https://github.com/incuna/drf-js#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-preset-es2015": "6.18.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "eslint": "3.9.1",
    "eslint-plugin-import": "2.2.0",
    "jasmine-core": "2.5.2",
    "jscs": "3.0.7",
    "karma": "1.3.0",
    "karma-browserify": "5.1.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "watchify": "3.7.0"
  }
}
